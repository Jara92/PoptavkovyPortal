<!DOCTYPE html>
<html lang="cs" dir="ltr">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{% block title %}Poptejsi.cz{% endblock %}</title>

    <link rel="apple-touch-icon" sizes="180x180"
          href="{{ asset("build/libs/laslesvpn_template/img/favicons/apple-touch-icon.png") }}"/>
    <link rel="icon" type="image/png" sizes="32x32"
          href="{{ asset("build/libs/laslesvpn_template/img/favicons/favicon-32x32.png") }}"/>
    <link rel="icon" type="image/png" sizes="16x16"
          href="{{ asset("build/libs/laslesvpn_template/img/favicons/favicon-16x16.png") }}"/>
    <link rel="shortcut icon" type="image/x-icon"
          href="{{ asset("build/libs/laslesvpn_template/img/favicons/favicon.ico") }}"/>
    <link rel="manifest" href="{{ asset("build/libs/laslesvpn_template/img/favicons/manifest.json") }}"/>
    <meta name="msapplication-TileImage"
          content="{{ asset("build/libs/laslesvpn_template/img/favicons/mstile-150x150.png") }}"/>
    <meta name="theme-color" content="#ffffff"/>

    <!-- CSS -->
    {{ encore_entry_link_tags('app') }}
    {% block styles %}{% endblock %}

    <!-- Scripts -->
    {{ encore_entry_script_tags('app') }}
    {% block scripts %}{% endblock %}
</head>

<body>
<div id="app">
    {% include "layouts/nav.html.twig" %}

    {% include "layouts/flash_messages.html.twig" %}

    <main class="main" id="top">
        {% block content %}
            <section class="pb-6">
                <div class="container">
                    <div class="row flex-center">
                        <div class="col-lg-6 col-md-5 order-md-1"><img class="img-fluid"
                                                                       src="{{ asset("build/libs/laslesvpn_template/img/illustrations/1.png") }}"
                                                                       alt=""/></div>
