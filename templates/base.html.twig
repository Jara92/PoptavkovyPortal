                                        <div>
                                            <p class="fw-bolder text-1000 mb-0">50+ </p>
                                            <p class="mb-0">Users </p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- end of .container-->

            </section>

            <section class="pt-4 pt-md-6">

                <div class="container">
                    <div class="row align-items-center">
